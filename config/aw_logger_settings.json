{
    "components": [
        {
            "type": "timestamp",
            "enabled": true
        },
        {
            "type": "level",
            "enabled": true
        },
        {
            "type": "tid",
            "enabled": true
        },
        {
            "type": "loc",
            "format": "[{file_name}:{function_name}:{line}]",
            "enabled": true
        },
        {
            "type": "color",
            "level_colors": {
                "debug": "white",
                "info": "cyan",
                "notice": "blue",
                "warn": "yellow",
                "error": "red",
                "fatal": "magenta"
            },
            "enabled": true
        },
        {
            "type": "msg",
            "enabled": true
        }
    ],
    "websocket": [
        {
            "url": "ws://localhost:1234",
            "message_deflate_en": false,
            "ping_interval": 30,
            "handshake_timeout": 5
        }
    ]
}
